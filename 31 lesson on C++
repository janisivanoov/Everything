#include <iostream>
#include <cstdlib>
#include <algorithm>
#include <cmath>
#include <string>
using namespace std;
int main(){
system("system chcp 1251 > nul");
char symbol;
int width , height;
cout<<"Enter a symbol to use:";
cin>>symbol;
cout<<"Enter a width:";
cin>>width;
cout<<"Enter a height:";
cin>>height;
for(int i = 0;i<height;i++){
for(int j=0;j<width;j++){
cout<<symbol;
}
cout<<endl;
}
system("pause > nul");
return 0;
}
