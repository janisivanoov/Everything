#include <iostream>
#include <cstdlib>
#include <vector>
using namespace std;
int main(){
system("chcp 1251 > nul");
int num=1==1?12:23;
int num=3;
swith(num){
case 1:
cout<<"1"<<endl;
break;
case 2:
cout<<"2"<<endl;
break;
case 3:
cout<<"3"<<endl;
break;
default:
cout<<"Not a typical number!"<<endl;
}
system("pause > nul");
return 0;
}
