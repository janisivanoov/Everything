#include <iostream>
using namespace std;
int getmax(int number1, int number2, int number3){
int result;
if(number1 >= number2 && number1 >= number3){
result = number1;
} else if(number2 >= number1 && number2 >= number3){
result = number2;
} else {
return = number3;
}
return result;
}
int main(){
cout<<getMax(2,5,10)<<endl;
return 0;
}
